  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Dev (NPM)",
      "runtimeVersion": "14.17.0",
      "cwd": "${workspaceFolder}",
      "outputCapture": "std",
      "env": {
        "PORT": "8080"
      },
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "dev"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
    },

  ]
}